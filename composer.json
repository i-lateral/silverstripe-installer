{
    "name": "i-lateral/silverstripe-installer",
    "description": "Customised SilverStripe Framework Installer",
    "require": {
        "php": ">=5.3.3",
		"silverstripe/framework": "3.3.*@stable",
        "silverstripe/cms": "3.3.*@stable",
		"silverstripe/reports": "3.3.*@stable",
		"silverstripe/siteconfig": "3.3.*@stable",
        "silverstripe/userforms": "3.*@stable",
        "silverstripe/googlesitemaps": "*",
        "silverstripe-australia/sitemap": "1.0.0",
        "i-lateral/silverstripe-sstweaks": "0.1.*",
        "i-lateral/silverstripe-compactnavigator": "1.1.*",
        "i-lateral/silverstripe-custommenus": "1.0.*",
        "i-lateral/silverstripe-themes-kube": "*",
        "i-lateral/silverstripe-socialnav": "*",
        "axllent/silverstripe-scaled-uploads": "v1.0@stable",
        "kinglozzer/metatitle": "1.0.*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~3.7@stable"
    },
    "config": {
        "process-timeout": 600
    },
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.3.x-dev"
		}
	},
	"prefer-stable": true,
    "minimum-stability": "dev"
}
