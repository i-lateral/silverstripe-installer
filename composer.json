{
    "name": "i-lateral/silverstripe-installer",
    "description": "Customised SilverStripe Framework Installer",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/i-lateral/silverstripe-simple.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/i-lateral/silverstripe-scaled-uploads"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "silverstripe/cms": "3.1.*@stable",
        "silverstripe/framework": "3.1.*@stable",
        "silverstripe-themes/simple": "*",
        "i-lateral/silverstripe-sstweaks": "dev-master",
        "axllent/silverstripe-scaled-uploads": "*",
        "i-lateral/silverstripe-carousel": "dev-master",
        "i-lateral/silverstripe-gallery": "dev-master",
        "i-lateral/silverstripe-compactnavigator": "dev-master",
        "i-lateral/silverstripe-custommenus": "dev-master",
        "silverstripe/googlesitemaps": "*",
        "silverstripe/userforms": "*",
        "silverstripe/blog": "*"
    },
    "config": {
        "process-timeout": 600	
    },
    "require-dev": {
        "silverstripe/docsviewer": "*",
        "silverstripe/behat-extension": "*",
        "silverstripe/buildtools": "*",
        "phpunit/phpunit": "3.7.*"
    },
    "scripts": {
        "post-install-cmd": "php ./framework/cli-script.php dev/build",
        "post-install-cmd": "chmod -R 755 *",
        "post-update-cmd": "php ./framework/cli-script.php dev/build",
        "post-update-cmd": "chmod -R 755 *"
    },
    "minimum-stability": "dev"
}
